{{#if description}}
/**
 * {{{getComment description}}}
 */
{{/if}}
export type {{{name}}} = {
    {{#each properties}}
    {{#if description}}
    /**
     * {{{getComment description}}}
     */
    {{/if}}
    {{>isReadOnly}}{{{name}}}{{>isRequired}}: {{>type parent=../name}};
    {{/each}}
}
{{#if enums}}
{{#unless @root.useUnionTypes}}

export namespace {{{name}}} {

    {{#each enums}}
    {{#if description}}
    /**
     * {{{getComment description}}}
     */
    {{/if}}
    export enum {{{name}}} {
        {{#each enum}}
        {{{name}}} = {{{value}}},
        {{/each}}
    }

    {{/each}}

}
{{/unless}}
{{/if}}
